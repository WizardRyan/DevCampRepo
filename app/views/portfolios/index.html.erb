<h1>Portfolio Items</h1>


<%= link_to "Create New Item", new_portfolio_path %>

<% @portfolio_items.each do |num| %>
<p><%= link_to num.title, portfolio_path(num.id) %></p>
<p><%= num.subtitle %></p>
<p><%= num.body %></p>
<%= image_tag num.thumbImage if !num.thumbImage.nil? %>
<%= link_to "Edit", edit_portfolio_path(num.id) %>
<%= link_to 'Delete Portfolio Item', portfolio_path(num.id), method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>