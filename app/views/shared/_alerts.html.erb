<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Ryan Andersen Portfolio", sticky: false) %>
  
<% elsif flash[:error] %>  
  <%= js add_gritter(flash[:error], title: "Ryan Andersen Portfolio", sticky: false) %>

<% else %>
    <%= js add_gritter(flash[:notice], title: "Ryan Andersen Portfolio", sticky: false) %>
<% end %>